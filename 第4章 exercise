#include "std_lib_facilities.h"

int main()
{
	cout << "primes\n";
	int max = 0;
	cout << " Enter max number.\n";
	cin >> max;

	vector<int> primes = { 2,3,5 };
	
	for (int i = 6; i <= max; ++i) {
		int j = 0;
		for (int x : primes) {
			if (i % x == 0) {
				j++;
			}
		}
			if (j == 0) { primes.push_back(i); }
	}
	for (int x : primes)cout << x << '\n';
	
	keep_window_open();
	return 0;
}


練習問題 13.

#include "std_lib_facilities.h"

int main()
{
	vector<int>number;
	vector<int>primes = { 2 };

	for (int i = 1; i <= 100; ++i) {
		number.push_back(i);
	}

	for (int i = 1; i < number.size(); ++i) {
		int j = 0;
		for (int x : primes) {
			if (number[i] % x == 0) { ++j; }
		}
		if (j == 0) { primes.push_back(number[i]); }
	}

	for (int x : primes)cout << x << '\n';

	keep_window_open();
	return 0;
}

練習問題 14.
#include "std_lib_facilities.h"

int main()
{
	vector<int>number;
	vector<int>primes = { 2 };
	int max = 0;

	cout << "Enter max number.\n";
	cin >> max;

	for (int i = 1; i <= max; ++i) {
		number.push_back(i);
	}

	for (int i = 1; i < number.size(); ++i) {
		int j = 0;
		for (int x : primes) {
			if (number[i] % x == 0) { ++j; }
		}
		if (j == 0) { primes.push_back(number[i]); }
	}

	for (int x : primes)cout << x << '\n';

	keep_window_open();
	return 0;
}


練習問題 15.

#include"std_lib_facilities.h"

int main()
{
	vector<int>number;
	vector<int>primes = { 2 };
	int max = 100000;
	int num = 0;

	cout << "How many prime number do you want to know?.\n";
	cin >> num;

	for (int i = 1; i <= max; ++i) {
		number.push_back(i);
	}

	for (int i = 1; i < number.size(); ++i) {
		int j = 0;
		for (int x : primes) {
			if (number[i] % x == 0) { ++j; }
		}
		if (j == 0) { 
			primes.push_back(number[i]);
			if (primes.size() == num)break;
		}
	}

	for (int x : primes)cout << x << '\n';

	keep_window_open();
	return 0;
}

練習問題 16.

#include "std_lib_facilities.h"

int main()
{
	vector<int> numbers;
	int tmp;
	int num1 = 0;
	int num2 = 0;
	int mode = 0;

	cout << "enter some integer.\n";

	while (cin >> tmp) {
		numbers.push_back(tmp);
	}

	for (int i = 0; i < numbers.size(); ++i) {
		for (int x : numbers) {
			if (numbers[i] == x) {
				++num1;
			}
		}
		if (num2 < num1) {
			mode = numbers[i];
			num2 = num1;
			num1 = 0;
		}
	}

	if(num1=num2){}
	else cout << "Mode is " << mode << ".\n";

	keep_window_open();
	return 0;
}


練習問題 17.

#include "std_lib_facilities.h"

int main()
{
	vector<string> words;
	string tmp = " ";
	int num1 = 0;
	int num2 = 0;
	string mode = " ";

	cout << "enter some strings.\n";

	while (cin >> tmp) {
		words.push_back(tmp);
	}

	for (int i = 0; i < words.size(); ++i) {
		for (string x : words) {
			if (words[i] == x) {
				++num1;
			}
		}
		if (num2 < num1) {
			mode = words[i];
			num2 = num1;
			num1 = 0;
		}
	}

	sort(words);
	cout << "Max is " << words[words.size() - 1] << ". Min is " << words[0] << ". ";
	if(num1=num2){}
	else cout << "Mode is " << mode << ".\n";

	keep_window_open();
	return 0;
}

練習問題 18.

#include "../../std_lib_facilities.h"

int main()
{
	cout << "Solve quadratic equations( ax^2 + bx + c = 0 ). Please enter a b c.\n";

	double a, b, c;
	double d=0;

	while (cin >> a >> b >> c) {
		d = sqrt(b*b - 4 * a*c);

		if (d < 0) {
			cout << "There's no real solution.\n";
		}
		else if (d == 0) {
			cout << "Solution is " << - b / 2 * a << ".\n";
		}
		else {
			cout << "Solution is " << (- b - d)/ 2 * a << " and " << (- b + d) / 2 * a << ".\n";
		}
	}

	keep_window_open();
	return 0;
}

練習問題 19.

#include "std_lib_facilities.h"

int main()
{
	vector<string>names;
	vector<int>scores;
	string name;
	int score;

	cout << "Please enter name and score.\n"
		<<"enter NoName and 0 for exit.\n";

	while (cin >> name >> score) {
		if (name == "NoName" && score == 0) { break; }

		if (names.size() == 0) {
			names.push_back(name);
			scores.push_back(score);
		}
		else {
			for (string x : names) {
				if (name == x) {
					error("same name.");
				}
			}
			names.push_back(name);
			scores.push_back(score);
		}


	}

	for (int i = 0; i < names.size(); ++i) {
		cout << names[i] << " " << scores[i] << "\n";
	}

	keep_window_open();
	return 0;
}


練習問題 20.		//前にもあったんだけど、最初のfor文をintに文字リテラル入れて終了させると自動的に、後ろのfor文も終了しちゃう。

#include "../../std_lib_facilities.h"

int main()
{
	vector<string> names(1);
	vector<int> scores(1);
	string name;
	int score;
	string thename = " ";

	cout << "最初に名前とスコアを入力してください。\n同じ名前を２つ以上使うことはできません。\n";

	for (int i = 0; cin >> name >> score; ++i) {
		int judge = 0;
		if (i == 0) {
			names[i] = name;
			scores[i] = score;
		}
		else {
			for (int j = 0; j <= i; ++j) {
				if(i == j){}
				else if (name == names[j]) {
					judge += 1;
					}
				else{}
			}
			
			if (judge != 0) {
				cout << "その名前は既に入力されています。\n";
				name = " ";
				score = 0;
				--i;
			}
			else {
				names.push_back(name);
				scores.push_back(score);
			}
		}
	}


	cin.clear();								//これでいいのかな？
	cin.ignore();								//調べたら見つかったんだけど。
	cout << "入力された中から、指定された名前の方の点数を出力します。\n";
	
	for (int y = 0; cin >> thename; ++y) {
		int judge = 0;
		for (int x = 0; x < names.size(); ++x) {
			if (thename == names[x]) {
				cout << thename << "さんの点数は" << scores[x] << "です。\n";
				++judge;
			}
		}
		if (judge == 0) {
			cout << "name not found.\n";
		}
	}

	return 0;
}

練習問題 21.

#include "../../std_lib_facilities.h"

int main()
{
	vector<string> names(1);
	vector<int> scores(1);
	string name;
	int score;
	int thescore = 0;

	cout << "最初に名前とスコアを入力してください。\n同じ名前を２つ以上使うことはできません。\n";

	for (int i = 0; cin >> name >> score; ++i) {
		int judge = 0;
		if (i == 0) {
			names[i] = name;
			scores[i] = score;
		}
		else {
			for (int j = 0; j <= i; ++j) {
				if(i == j){}
				else if (name == names[j]) {
					judge += 1;
					}
				else{}
			}
			
			if (judge != 0) {
				cout << "その名前は既に入力されています。\n";
				name = " ";
				score = 0;
				--i;
			}
			else {
				names.push_back(name);
				scores.push_back(score);
			}
		}
	}
	cin.clear();
	cin.ignore();
	cout << "先に入力された中から、指定された点数の方の名前を出力します。\n";
	
	for (int y = 0; cin >> thescore; ++y) {
		int judge = 0;
		for (int x = 0; x < names.size(); ++x) {
			if (thescore == scores[x]) {
				cout <<"点数が"<< thescore << "の方は" << names[x] << "さんです。\n";
				++judge;
			}
		}
		if (judge == 0) {
			cout << "score not found.\n";
		}
	}

	return 0;
}
